module.exports = {

	// ----------------------------------------------------------------------------
	// ----------------------------------------------------------------------------
	conexion: null, 

	// ----------------------------------------------------------------------------
	// prueba()
	// ----------------------------------------------------------------------------
	f: async function ( args ) {
		var self = this
		
		return new Promise( function( resolver, rechazar) {
			setTimeout( function() { // simulo que llamo función asíncrona
				resolver( args )
			}, 200 )
		})
		
	} // ()
} // exports

